{
  "name": "edge",
  "version": "0.0.1",
  "description": "api gateway for probolinggo-dev",
  "main": "index.js",
  "license": "MIT",
  "private": false,
  "scripts": {
    "create-env": "printenv > .env",
    "build": "node build.js",
    "start": "node -r module-alias/register ./dist --env=production",
    "gcp-build": "node build.js",
    "test": "jest",
    "dev": "nodemon"
  },
  "_moduleAliases": {
    "src": "dist"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, json, graphql",
    "ignore": [
      "src/dist"
    ],
    "exec": "ts-node -r tsconfig-paths/register ./src"
  },
  "dependencies": {
    "apollo-server-express": "^2.15.1",
    "apollo-server-testing": "^2.15.1",
    "camelcase-keys": "^6.2.2",
    "class-validator": "^0.12.2",
    "dotenv": "^8.2.0",
    "error-stack-parser": "^2.0.6",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "graphql": "^15.2.0",
    "graphql-tag": "^2.10.3",
    "module-alias": "^2.2.2",
    "qs": "^6.9.4",
    "reflect-metadata": "^0.1.13",
    "superagent": "^5.3.1",
    "type-graphql": "^1.0.0-rc.3",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/glob": "^7.1.2",
    "@types/jest": "^26.0.3",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.14",
    "@types/superagent": "^4.1.8",
    "@types/winston": "^2.4.4",
    "fs-extra": "^9.0.1",
    "jest": "^26.1.0",
    "nodemon": "^2.0.4",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "tsc": "^1.20150623.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.6"
  }
}
